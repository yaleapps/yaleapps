{
	"name": "superlatives",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"description": "A web app that enables users to pick favorite courses, and brings an alternative view to CourseTable for mobile and desktop view.",
	"productName": "Course Superlatives",
	"author": "Braden Wong <13159333+braden-w@users.noreply.github.com>",
	"engines": {
		"node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
		"npm": ">= 6.13.4",
		"yarn": ">= 1.21.1"
	},
	"scripts": {
		"lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
		"test": "echo \"No test specified\" && exit 0",
		"update-types": "supabase gen types typescript --project-id \"$PROJECT_REF\" --schema public > database.types.ts",
		"dev": "quasar dev",
		"build": "quasar build",
		"postinstall": "quasar prepare",
		"deploy": "wrangler pages deploy"
	},
	"dependencies": {
		"@quasar/extras": "^1.16.4",
		"@repo/constants": "workspace:*",
		"@supabase/supabase-js": "^2.49.4",
		"@tailwindcss/postcss": "^4.1.3",
		"@tanstack/vue-query": "^5.73.3",
		"arktype": "^2.1.9",
		"fuse.js": "^7.1.0",
		"pinia": "^3.0.1",
		"pinia-plugin-persistedstate": "^4.2.0",
		"quasar": "^2.16.0",
		"tailwindcss": "^4.0.6",
		"vue": "^3.4.18",
		"vue-router": "^4.0.12"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.12.0",
		"@cloudflare/vite-plugin": "^1.0.5",
		"@quasar/app-vite": "^2.1.0",
		"@types/node": "^22.14.1",
		"autoprefixer": "^10.4.2",
		"eslint": "^9.24.0",
		"eslint-plugin-format": "^1.0.1",
		"globals": "^15.12.0",
		"prettier": "^3.3.3",
		"supabase": "^2.20.12",
		"typescript": "^5.7.3",
		"vite-plugin-checker": "^0.9.0",
		"vue-tsc": "^2.0.29",
		"wrangler": "^4.9.1"
	}
}
